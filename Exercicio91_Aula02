package Exercicio91_Aula02;

class Aula02 {

	public Aula02() {
		
	}

	public static void main(String[] args) {
		Caneta c1 = new Caneta();
		c1.modelo = "BIC crital";
		c1.cor = "Azul";
		//c1.ponta = 0.5f;
		c1.carga = 80;
		//c1.tampada = false;
		c1.tampar();
		c1.rabiscar();
		c1.status();
	}

}
