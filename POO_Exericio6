package POO_Exericio6;

public class Ex06 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] V = {10, 2, 5, 25, 1, 3, 14};
		for(int i = 0; i < V.length; i++) {
			if(V[i] % 2 != 0) {
				V[i]++;		
			}			
		}		
		for(int i = 0; i < V.length; i++) {
			System.out.println(V[i]);			
		}		
	}

}
