package POO_Exercicio5;

public class Ex05 {	
	public static void main (String [] args) {
		int [] V = {10, 2, 5, 25, 1, 3, 14};
		int aux = 0;
		for(int i = 0; i < V.length; i++) {
			for(int j = 0; j < V.length; j++) {
				if(V[i] < V[j]) {
					aux = V[i];
					V[i] = V[j];
					V[j] = aux;	
				}				
			}			
		}
		for(int i = 0; i < V.length; i++) {
			System.out.println(V[i]);			
		}
	}
}
